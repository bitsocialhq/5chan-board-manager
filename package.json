{
  "name": "5chan-board-archiver",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "5chan-archiver": "dist/cli.js"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "vitest run",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "start": "node --env-file=.env dist/cli.js"
  },
  "dependencies": {
    "@plebbit/plebbit-js": "https://github.com/plebbit/plebbit-js",
    "env-paths": "^4.0.0"
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  }
}
